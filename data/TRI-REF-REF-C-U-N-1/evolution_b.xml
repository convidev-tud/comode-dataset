<?xml version="1.0"?>
<Evolution xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns="http://mergebench.org/ns"
          xsi:schemaLocation="http://mergebench.org/ns ../../meta/XSL/operations.xsd">

    <SemanticEdit index="0">
        <Semantic>
            <Category>REFINE</Category>
            <Description>
                Refine the engine by adding properties.
            </Description>
        </Semantic>
        <Operation index="0">
            <DeleteProperty nodeName="Engine" propertyName="powerConsumption" />
        </Operation>
        <Operation index="1">
            <DeleteProperty nodeName="Engine" propertyName="weight" />
        </Operation>
        <Operation index="2">
            <AddProperty nodeName="Engine" propertyName="powerConsumption=200KW" />
        </Operation>
        <Operation index="3">
            <AddProperty nodeName="Engine" propertyName="weight=200kg" />
        </Operation>
    </SemanticEdit>

</Evolution>