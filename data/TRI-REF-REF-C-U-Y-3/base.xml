<?xml version="1.0"?>
<Graph xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns="http://mergebench.org/ns"
          xsi:schemaLocation="http://mergebench.org/ns ../../meta/XSL/graph.xsd">

    <Group name="OOP">
        <Node name="Package" />
        <Node name="Class" />
        <Node name="Signature" />
        <Node name="Method" />
        <Node name="Return" />
        <Node name="Declaration" />
        <Node name="ParameterDefinition" />
        <Node name="Name" />
        <Node name="Datatype" />
    </Group>

    <DirectedEdge start="Package" end="Class" semantics="contains" endmult="*"/>
    <DirectedEdge start="Class" end="Signature" semantics="contains" endmult="1"/>
    <DirectedEdge start="Class" end="Method" semantics="contains" endmult="*"/>
    <DirectedEdge start="Method" end="Return" semantics="contains" endmult="1"/>
    <DirectedEdge start="Method" end="Declaration" semantics="contains" endmult="1"/>
    <DirectedEdge start="Declaration" end="ParameterDefinition" semantics="contains" endmult="*" />
    <DirectedEdge start="ParameterDefinition" end="Name" semantics="contains" endmult="1"/>
    <DirectedEdge start="ParameterDefinition" end="Datatype" semantics="contains" endmult="1"/>
</Graph>