<?xml version="1.0"?>
<Graph xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns="http://mergebench.org/ns"
          xsi:schemaLocation="http://mergebench.org/ns ../../meta/XSL/graph.xsd">

    <Group name="University">
        <Node name="Chair" />
        <Node name="Employee" />
        <Node name="Scientist" />
        <Node name="Secretary" />
        <Node name="Professor" />
        <Node name="PostDoc" />
        <Node name="Researcher" />
    </Group>

    <DirectedEdge start="Chair" end="Employee" semantics="contain" endmult="2..*"/>
    <DirectedEdge start="Employee" end="Scientist" semantics="contain" endmult="*"/>
    <DirectedEdge start="Employee" end="Secretary" semantics="contain" endmult="1"/>
    <DirectedEdge start="Employee" end="Professor" semantics="contain" endmult="1"/>
    <DirectedEdge start="Scientist" end="PostDoc" semantics="contain" endmult="*"/>
    <DirectedEdge start="Scientist" end="Researcher" semantics="contain" endmult="*"/>
</Graph>